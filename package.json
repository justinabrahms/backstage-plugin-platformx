{
  "name": "@justinabrahms/backstage-plugin-platformx",
  "version": "0.3.4",
  "description": "Backstage plugin for tracking user page visits with PlatformX by DX",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/justinabrahms/backstage-plugin-platformx.git",
    "directory": "."
  },
  "keywords": [
    "backstage",
    "plugin",
    "platformx",
    "dx",
    "analytics",
    "tracking"
  ],
  "author": "Justin Abrahms",
  "backstage": {
    "role": "frontend-plugin",
    "pluginId": "platformx",
    "pluginPackages": [
      "@justinabrahms/backstage-plugin-platformx"
    ]
  },
  "exports": {
    ".": "./src/index.ts",
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "package.json": [
        "package.json"
      ]
    }
  },
  "dependencies": {
    "@backstage/frontend-plugin-api": "^0.8.0",
    "react-use": "^17.4.0"
  },
  "peerDependencies": {
    "react": "^18.0.0"
  },
  "devDependencies": {
    "@backstage/cli": "^0.28.0",
    "@types/react": "^18.0.0",
    "typescript": "^5.0.0"
  },
  "sideEffects": false,
  "scripts": {
    "build": "backstage-cli package build",
    "tsc": "tsc --build --force",
    "clean": "backstage-cli package clean",
    "prepack": "backstage-cli package prepack",
    "postpack": "backstage-cli package postpack"
  },
  "files": [
    "dist",
    "config.d.ts"
  ],
  "configSchema": "config.d.ts"
}
